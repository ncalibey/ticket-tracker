<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Assignee</th>
      <th>Status</th>
      <th>Tags</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% tickets.each do |ticket| %>
    <tr>
      <td><%= link_to ticket.name, ticket %></td>
      <td><%= ticket.assignee.try(:name) %></td>
      <td>
        <%= ticket.status.titleize %>
      </td>
      <td><%= tag_list(ticket) %></td>
      <td>
        <%= link_to 'Edit', edit_ticket_path(ticket) %>
        <%= link_to 'Destroy', ticket, method: :delete, data: { confirm: 'Are you sure?' } %>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>
